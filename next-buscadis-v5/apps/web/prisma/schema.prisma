generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Clasificado {
  id          String   @id @default(cuid())
  titulo      String
  descripcion String   @db.Text
  precio      Float
  imagen      String?
  ubicacion   String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
} 